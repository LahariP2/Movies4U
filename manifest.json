{
    "manifest_version" : 2,
    "name" : "movies4u",
    "version" : "1.0",
    "description" : "Movies4U",
    "browser_action" : {
        "default_popup" : "popup.html"
    },
  
    "options_page" : "options.html",
  
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    }, 
  
    "permissions": [
        "storage",
        "notifications",
        "contextMenus"
    ]

 }
 